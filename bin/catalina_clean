rm -f *.lst 
rm -f *.debug 
rm -f *.dbg
popd
pushd $TMP_LCCDIR/demos/interrupts
rm -f *.bin
rm -f *.binary
rm -f *.eeprom 
rm -f *.lst 
rm -f *.debug 
rm -f *.dbg
popd
pushd $TMP_LCCDIR/demos/loader
rm -f *.bin
