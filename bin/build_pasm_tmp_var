#!/bin/bash
##!/bin/bash -login
#
# build a string in TMP_VAR consisting of -D $1 -D $2 -D $3 (etc)
#
unset TMP_VAR
if [[ "$1" != "" ]]; then
TMP_VAR="-D $1"; export TMP_VAR;
fi
if [[ "$2" != "" ]]; then
TMP_VAR="$TMP_VAR -D $2"; export TMP_VAR;
fi
if [[ "$3" != "" ]]; then
TMP_VAR="$TMP_VAR -D $3"; export TMP_VAR;
fi
if [[ "$4" != "" ]]; then
TMP_VAR="$TMP_VAR -D $4"; export TMP_VAR;
fi
if [[ "$5" != "" ]]; then
TMP_VAR="$TMP_VAR -D $5"; export TMP_VAR;
fi
if [[ "$6" != "" ]]; then
TMP_VAR="$TMP_VAR -D $6"; export TMP_VAR;
fi
if [[ "$7" != "" ]]; then
TMP_VAR="$TMP_VAR -D $7"; export TMP_VAR;
fi
if [[ "$8" != "" ]]; then
TMP_VAR="$TMP_VAR -D $8"; export TMP_VAR;
fi
if [[ "$9" != "" ]]; then
TMP_VAR="$TMP_VAR -D $9"; export TMP_VAR;
fi
#echo $TMP_VAR;

