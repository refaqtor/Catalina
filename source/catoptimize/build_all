./CompileOptimizer cmmopt_10
./CompileOptimizer cmmopt_11
./CompileOptimizer cmmopt_12
./CompileOptimizer cmmopt_13
./CompileOptimizer cmmopt_14
gcc -o cmmoptimize cmmoptimize.c
